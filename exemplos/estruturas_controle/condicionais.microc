// Exemplo 1: Estruturas condicionais if/else
int main() {
    int idade = 18;
    
    if (idade >= 18) {
        printf(1); // Maior de idade
    } else {
        printf(0); // Menor de idade
    }
    
    int nota = 85;
    if (nota >= 90) {
        printf(4); // A
    } else if (nota >= 80) {
        printf(3); // B
    } else if (nota >= 70) {
        printf(2); // C
    } else {
        printf(1); // D/F
    }
    
    return 0;
}
